/**
 * Duplicate
 */
public class Duplicate {
    public static void main(String[] args){
        int[] l={1,2,3,4,5,2,1};
        for (int i=0;i<l.length;i++){
            for(int j=i+1;j<l.length;j++){
                if (l[i]==l[j])
                System.out.println(l[j]);
       
        }

     }

    }

    
}
